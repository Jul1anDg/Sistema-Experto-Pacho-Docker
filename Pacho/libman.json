{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.6.4",
      "destination": "wwwroot/lib/jquery",
      "files": [
        "jquery.min.js",
        "jquery.min.map"
      ]
    },
    {
      // Quitamos "provider": "unpkg" y usamos cdnjs
      "library": "jquery-ui@1.13.2",
      "destination": "wwwroot/lib/jquery-ui",
      "files": [
        "jquery-ui.min.js",
        "themes/base/jquery-ui.min.css"
      ]
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/lib/jquery-validation",
      "files": [
        "jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "bootstrap@5.3.2",
      "destination": "wwwroot/lib/bootstrap",
      "files": [
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.min.js"
      ]
    }
  ]
}
